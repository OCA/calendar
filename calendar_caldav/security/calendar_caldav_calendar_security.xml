<odoo>

    <record id="rule_calendar_caldav_calendar" model="ir.rule">
        <field name="name">User Caldav Calendar</field>
        <field name="model_id" ref="model_calendar_caldav_calendar"/>
        <field name="domain_force">[('user_id', '=', user.id)]</field>
        <field name="groups" eval="[
            (4, ref('base.group_user'))
        ]"/>
    </record>

</odoo>

<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="portal_wizard_form_view_inherit" model="ir.ui.view">
            <field name="name">portal.wizard.form.view.inherit</field>
            <field name="model">portal.wizard</field>
            <field name="inherit_id" ref="portal.wizard_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='user_ids']/tree" position="inside">
                    <field name="booking_from_portal" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>
